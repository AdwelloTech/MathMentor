{
  "name": "mathmentor-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently -n web,api -c blue,magenta \"npm run dev:web\" \"npm run dev:api\"",
    "dev:web": "vite --config apps/web/vite.config.ts",
    "dev:api": "tsx apps/api/src/index.ts",
    "build": "vite build --config apps/web/vite.config.ts",
    "preview": "vite preview --config apps/web/vite.config.ts",
    "type-check": "tsc --noEmit",
    "dev:api:new": "tsx apps/api/_new/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
